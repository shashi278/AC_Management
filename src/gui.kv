#-------------------ProfilePage---------------------#
<ProfilePage>:
	name: 'profilepage'
	FloatLayout:
		canvas:
			Color:
				rgba: C(root.color)
			Rectangle:
				size: self.size
				pos: self.pos

		BoxLayout:
			orientation: 'vertical'
			spacing: dp(5)

			BoxMinimumHeight:
				padding:0,2,0,0

				Widget:
					size_hint: None, 1
					width: 20

				MDIconButton:
					size_hint: None, None
					size: 50,50
					icon: 'arrow-left'
					md_bg_color: app.theme_cls.primary_color
					theme_text_color: "Custom"
					text_color: [1,1,1,1]
					on_release:
						if root.check_edits(root.ids.rv.children[0]):\
						app.root.transition= FallOutTransition();app.root.current= "userScreen"
				

			BoxMinimumHeight:
